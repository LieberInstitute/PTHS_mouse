dat =read_excel('MOG_ICC_data.xlsx')
key =read_excel("MOG_Blind_Key.xlsx")
#########################
#names(dat)
#[1] "Animal" "Slice"  "Mean"   "Min"    "Max" 
###########
#names(key)
#[1] "ACTUAL ANIMAL ID "     "LABELED AS FOR BLIND " "GENOTYPE" 
dat$Genotype = factor(key[match(toupper(dat$Animal),toupper(key[,2])),3],
                      levels = c('W','H'),labels = c("WT",'Het'))
library(lme4)
library(lmerTest)
boxplot(Mean~Genotype, data = dat,main = 'Adult mice DG Mog ICC')
summary(lmer(data = dat,Mean~Genotype+(1|Animal)))
boxplot(Mean~Genotype/255, data = dat,main = 'Adult mice DG Mog ICC',ylab= 'Normalized Intensity')
boxplot(Mean/255~Genotype, data = dat,main = 'Adult mice DG Mog ICC',ylab= 'Normalized Intensity')
beeswarm(Mean/255~Gentype,data = dat,add = T)
library(beeswarm)
beeswarm(Mean/255~Gentype,data = dat,add = T)
beeswarm(Mean/255~Genotype,data = dat,add = T)
boxplot(Mean/255~Genotype, data = dat,main = 'Adult mice DG Mog ICC',ylab= 'Normalized Intensity')
beeswarm(Mean/255~Genotype,data = dat,add = T,pwbg = factor(Animal))
dev.off()
boxplot(Mean/255~Genotype, data = dat,main = 'Adult mice DG Mog ICC',ylab= 'Normalized Intensity')
beeswarm(Mean/255~Genotype,data = dat,add = T,pwbg = factor(Animal))
beeswarm(Mean/255~Genotype,data = dat,add = T,pch = 21,pwbg = factor(Animal))
